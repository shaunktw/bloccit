<div class="row">
  <div class="col-md-8">
    <h1> Trending</h1>
    <p class="lead"> Newest places this week </p>
    <div id="tag_cloud">
    Tag Cloud:
     <% tag_cloud Post.tag_counts_on(:tags, :limit => 5, :order => "count desc"), %w[s m l] do |tag, css_class| %>
     <%= link_to tag.name, tag_path(tag.name), class: css_class %>
     <% end %>
    </div>
    <%= render partial: 'posts/post', collection: @posts %>
      <%= will_paginate @posts %>
  </div>
  <div class="col-md-4">
  </div>
</div>





